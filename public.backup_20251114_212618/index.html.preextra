  <script>
  (function(){
    function fixProxy(el){
      try{
        var url = new URL(el.src, window.location.origin);
        var orig = url.searchParams.get("url");
        if(orig && el.dataset.proxyFixed !== "1"){
          el.src = orig;
          el.dataset.proxyFixed = "1";
        }
      }catch(e){}
    }
    function fixAll(){
      var list = document.querySelectorAll(img[src*=/api/proxy?url=],video[src*=/api/proxy?url=]);
      list.forEach(fixProxy);
    }
    document.addEventListener("DOMContentLoaded", fixAll);
    window.addEventListener("load", fixAll);
    setInterval(fixAll, 1500);
  })();
  </script>
